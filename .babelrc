{
    "presets": [
        [
            "env", {
            "modules": false,
            "targets": {
                "node": "current",
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }
        ],
        "stage-2"
    ],
    "plugins": [
        "transform-vue-jsx",
        "transform-runtime"
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "env", {
                    "targets": {
                        "node": "current"
                    }
                }
                ],
                "stage-2"
            ]
        }
    }
}
